  "scripts": {
    "main": "node -e \"const fs = require('fs');const path = require('path');const currentDir = __dirname;const server = path.join(currentDir, 'dist');const directories = {files: fs.readdirSync(server).filter((file) => {return fs.statSync(path.join(server, file)).isFile();}), directories: fs.readdirSync(server).filter((file) => {return fs.statSync(path.join(server, file)).isDirectory();})};console.log(directories);\"",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "start": "nest start",
    "prod": "node dist/src/server/main.js",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "vitest --config ./vitest.config.e2e.mts"
  },
